            gazetedergiPanel.Visible = true;
            hijyenurunlerıPanel.Visible = false;
            unlumamullerPanel.Visible = false;
            meyvesebzePanel.Visible = false;
            iceceklerPanel.Visible = false;
            temelgidapanel.Visible = false;
            atistirmalikPanel.Visible = false;
            evurunleriPanel.Visible =false;
            evcilhayvanPanel.Visible = false;
        }

