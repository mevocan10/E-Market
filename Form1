            meyvesebzePanel.Visible = false;
            iceceklerPanel.Visible = false;
            temelgidapanel.Visible = false;
            atistirmalikPanel.Visible = false;
        }

        private void guna2GradientButton1_Click(object sender, EventArgs e)
            
        {
            if(odenecektutarr.Text=="Sepetiniz Boş")
                MessageBox.Show("Lütfen sepetinize bir kaç şey ekleyin.", "Program");
            else
            {
                if (UserControl1.x >= 1)
                    MessageBox.Show("Siparişiniz başarıyla oluşturuldu.Toplam tutar : "+UserControl1.x, "Program");
                else
                    MessageBox.Show("Lütfen sepetinize bir kaç şey ekleyin.", "Program");
            }
        }


    }
